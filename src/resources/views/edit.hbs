<div class="modal-dialog">
          <div class="modal-content">
            <form action="/admin" method="POST">
              <div class="modal-header">
                <h4 class="modal-title">Edit Employee</h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
              </div>
              <div class="modal-body">
                {{!-- {{#each statistical}} --}}
                <div class="form-group">
                  <label>Bệnh Nhân</label>
                  <input type="text" class="form-control" value="{{patient}}" id="patient" name="patient" required>
                </div>
                <div class="form-group">
                  <label>Tuổi</label>
                  <input type="text" class="form-control" value="{{age}}" id="age" name="age" required>
                </div>
                <div class="form-group">
                  <label>Địa chỉ</label>
                  <input type="text" class="form-control" value="{{address}}" id="address" name="address"  required>
                </div>
                <div class="form-group">
                  <label>Tình Trạng</label>
                  <input type="text" class="form-control" value="{{state}}" id="state" name="state" required>
                </div>
                <div class="form-group">
                  <label>Quốc tịch</label>
                  <input type="text" class="form-control" value="{{nationality}}" id="nationality" name="nationality" required>
                </div>
                {{!-- {{/each}} --}}
              </div>
              <div class="modal-footer">
                {{!-- <input type="button" class="btn btn-default" data-dismiss="modal" value="Cancel"> --}}
                <a href="/admin" type="button" class="btn btn-default" data-dismiss="modal" value="Cancel">Cancel</a>
                <button type="submit" class="btn btn-info">Save</button>
              </div>
            </form>
          </div>
        </div>